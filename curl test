curl -v -X $'GET'     -H $'Host: www.site.io' -H $'Connection: close' -H $'Cache-Control: max-age=0' -H $'Upgrade-Insecure-Requests: 1' -H $'User-Agent: curl' -H $'Accept: text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.9' -H $'Sec-Fetch-Site: same-site' -H $'Sec-Fetch-Mode: navigate' -H $'Sec-Fetch-User: ?1' -H $'Sec-Fetch-Dest: document' -H $'Referer: https://login.site.io/' -H $'Accept-Encoding: gzip, deflate' -H $'Accept-Language: id,en-US;q=0.9,en;q=0.8' -H $'Cookie: COOKIE'     $'https://www.site.io/siteio/information/users' | xmllint --format - | grep "@"
